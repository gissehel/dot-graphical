bindsym $mod+w exec rofi -show window -lines 25 -opacity 85
# bindsym $mod+d exec rofi -show run

bindsym $mod+d exec rofi -show run -lines 25 -opacity 85
# -width 100 -padding 500 -opacity "85" -bw 0 
bindsym $mod+Shift+d exec rofi -show drun -lines 25 -opacity 85

bindsym $mod+c exec rofi -modi "clipboard:greenclip print" -show clipboard -lines 25 -opacity 85


